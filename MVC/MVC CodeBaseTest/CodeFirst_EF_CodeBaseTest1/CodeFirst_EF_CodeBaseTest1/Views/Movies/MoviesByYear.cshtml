@model IEnumerable<CodeFirst_EF_CodeBaseTest1.Models.Movie>
@{ ViewBag.Title = "MoviesByYear"; }
<h2>Movies Released in Year</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MovieName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfRelease)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MovieName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DateOfRelease)
    </td>
    <td>
        @Html.ActionLink("Details", "Details", new { id = item.Mid }) |
        <!-- Add more actions if needed -->
    </td>
</tr>}
</table>